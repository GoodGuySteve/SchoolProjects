-- concrete syntax example 2:

testedout(alice, 16).
taken(alice, 24).
{Student} ready(Student, 138) :-
    (taken(Student, 16) ; testedout(Student, 16)),
    (taken(Student, 24) ; testedout(Student, 24)),
    (taken(Student, 40) ; testedout(Student, 40)).

? {Class} (taken(alice, 40) => (ready(alice, Class))).
